{
	"name": "wikibase/query-engine",
	"type": "library",
	"description": "Answers Ask queries against a collection of Wikibase entities",
	"keywords": [
		"Wikibase",
		"QueryEngine",
		"Wikidata"
	],
	"homepage": "https://github.com/wmde/WikibaseQueryEngine",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Jeroen De Dauw",
			"email": "jeroendedauw@gmail.com",
			"homepage": "http://jeroendedauw.com",
			"role": "Developer"
		}
	],
	"support": {
		"irc": "irc://irc.freenode.net/wikidata"
	},
	"require": {
		"php": ">=5.3.0",
		"data-values/data-values": "~0.1",
		"data-values/iri": "~0.1",
		"data-values/number": "~0.1",
		"data-values/geo": "~0.1",
		"data-values/time": "~0.6",
		"ask/ask": "~1.0",
		"wikibase/data-model": "~0.7.4",
		"doctrine/dbal": "~2.4.2"
	},
	"require-dev": {
		"ext-sqlite3": "*"
	},
	"autoload": {
		"files" : [
			"WikibaseQueryEngine.php"
		],
		"psr-4": {
			"Wikibase\\QueryEngine\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.1.x-dev"
		}
	}
}
